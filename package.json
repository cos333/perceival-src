{
  "name": "perceival-server",
  "version": "1.0.0",
  "engines": {
    "node": "6.9.x"
  },
  "scripts": {
    "start": "node server",
    "dev": "concurrently \"cd client && PORT=3000 npm start\" \"cd server && PORT=3001 node index.js\"",
    "heroku-postbuild": "cd client/ && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "body-parser": "^1.17.1",
    "concurrently": "^3.4.0",
    "express": "^4.14.1"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
